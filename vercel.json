{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    },
      "src": "/generate-mindmap", 
      "dest": "/api/generate-mindmap.js"
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
